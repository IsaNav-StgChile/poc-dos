<h1>Homes#index</h1>

<% if user_signed_in? %>
<h2>Bienvenido <%= current_user.first_name %></h2>
<% end %>

<table> 
    <tbody> 
        <% if @photos %> 
            <td>Fotos:</td> 
            <td><%= @photos.size %></td> 
        <% end %>
    </tbody> 
</table> 

<% if @photos %>

<div class="container photos row">
    <% @photos.each do |photo| %>
    <div class="col-md-3">
        <%= render 'photos/photo', photo: photo %>
    </div>
    <% end %>
</div>

<% end %>	